{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "ZORBOX Static Analyzer Output",
  "type": "object",
  "properties": {
    "file": {
      "type": "object",
      "properties": {
        "name": {"type": "string"},
        "size": {"type": "integer"},
        "type": {"type": "string"}
      },
      "required": ["name", "size", "type"]
    },
    "hashes": {
      "type": "object",
      "properties": {
        "sha256": {"type": "string"},
        "md5": {"type": "string"}
      }
    },
    "heuristics": {"type": "object"},
    "details": {"type": "object"},
    "strings_sample": {"type": "string"},
    "yara_hits": {"type": "array", "items": {"type": "string"}}
  },
  "required": ["file", "hashes", "heuristics", "details"]
}
